{
  "name": "custodi",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "dev": "concurrently \"yarn server\" \"yarn client:start\"",
    "client:start": "cd client && yarn start",
    "client:install": "cd client && yarn",
    "lint": "npx eslint --fix *.js test *.js",
    "client:lint": "cd client &&  yarn lint",
    "lint:all": "yarn lint && yarn client:lint",
    "test": "jest",
    "client:test": "cd client && yarn test",
    "test:all": "yarn test && yarn client:test",
    "client:precommit": "cd client && yarn precommit",
    "clean": "rm -rf node_modules && cd client && rm -rf node_modules",
    "heroku-postbuild": "cd client && yarn && yarn build"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ]
  },
  "lint-staged": {
    "**/*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  }
}
